<div class="page" id=<%= page.id %>>
    <p>Page:</p>
    <ol>
        <% page.tab_groups.each do |group| %>
            <%= render 'shared/error_messages', object: group.object%>
            <li>Group: <%= render partial: "tab_group", locals: {group: group} %></li>
        <% end %>
    </ol>
</div>
