<!DOCTYPE html>
<html>
<head>
  <title>Labrats</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <% ["page"].each do |template_name| %>
