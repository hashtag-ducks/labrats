<div class="user-page">
    <%= gravatar_for @user %>
    <h1><%= @user.name %></h1>
    <%= @user.organization %> <br/>
    <% if current_user? @user %>
      <%= link_to "Your notebooks", user_notebooks_url(@user) %>
    <% end %>
  <h2>Recent Activity</h2>
  <div id="user-feed">
    <%= render 'feed' %>
  </div>
</div>
