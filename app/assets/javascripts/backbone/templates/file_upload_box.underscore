<div class="box file-upload-box" id="file-upload-box-<%= id %>">
  <form method="POST" name="box" action="/boxes/<%= id %>/files" enctype="multipart/form-data">
    <input type="file" name="file" class="file-upload-box-file" />
  </form>
  <button href="#" class="save-box btn btn-default">Upload File</button>
</div>
