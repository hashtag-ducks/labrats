<div class="box text-box" id="text-box-<%= id %>">
  <h5>Box <%= id %>: <%= name %></h5>
  <input type="text" class="text-box-name" val="<%= name %>" />
  <textarea class="text-box-content"><%= content %></textarea>
  <a href="#" class="save-box">Save Box</a>
  <a href="#" class="delete-box">Delete Box</a>
</div>
